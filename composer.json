{
    "name": "hakimrazalan/enginemailer-php",
    "description": "EngineMailer PHP REST SDK (forked + Symfony 6/7 compat)",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "HakimRazalan\\EngineMailer\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Hakim Razalan",
            "email": "akimrr@gmail.com"
        }
    ],
    "require": {
        "symfony/http-client": "^6.4 || ^7.0",
        "laravie/codex-filter": "^2.1",
        "laravie/codex": "^6.2"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "require-dev": {
        "php-http/guzzle7-adapter": "^1.1",
        "rector/rector": "^2.0"
    }
}
